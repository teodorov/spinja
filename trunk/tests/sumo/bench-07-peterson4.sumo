/* bench-07-peterson4.sumo - Peterson's algorithm for 4 processes   */
/* generated by: m4 -DN=4 peterson.m4 > peterson4.prom              */

short mutex;
short turn0;
short turn1;
short turn2;
short turn3;

short flag0;
short flag1;
short flag2;
short flag3;

active proctype P0 () {
    short j;
    short k;
    do  
    ::  /* again */
        k = 0; /* count max N-1 rounds of competition */
        do
        ::  k < 4-1 ->
                flag0 = k;
                if
                :: k == 0 -> turn0 = 0;
                :: k == 1 -> turn1 = 0;
                :: k == 2 -> turn2 = 0;
                :: k == 3 -> turn3 = 0;
                fi;
                
                j = 0; /* for all j != _pid */
                do
                ::  j == 0 -> j = j+1;
                ::  j != 0 -> 
                    if
                    ::  j < 4 ->
                            if
                            :: j == 0 && k == 0 -> flag0 < k || turn0 != 0; j = j+1;
                            :: j == 0 && k == 1 -> flag0 < k || turn1 != 0; j = j+1;
                            :: j == 0 && k == 2 -> flag0 < k || turn2 != 0; j = j+1;
                            :: j == 0 && k == 3 -> flag0 < k || turn3 != 0; j = j+1;
                            :: j == 1 && k == 0 -> flag1 < k || turn0 != 0; j = j+1;
                            :: j == 1 && k == 1 -> flag1 < k || turn1 != 0; j = j+1;
                            :: j == 1 && k == 2 -> flag1 < k || turn2 != 0; j = j+1;
                            :: j == 1 && k == 3 -> flag1 < k || turn3 != 0; j = j+1;
                            :: j == 2 && k == 0 -> flag2 < k || turn0 != 0; j = j+1;
                            :: j == 2 && k == 1 -> flag2 < k || turn1 != 0; j = j+1;
                            :: j == 2 && k == 2 -> flag2 < k || turn2 != 0; j = j+1;
                            :: j == 2 && k == 3 -> flag2 < k || turn3 != 0; j = j+1;
                            :: j == 3 && k == 0 -> flag3 < k || turn0 != 0; j = j+1;
                            :: j == 3 && k == 1 -> flag3 < k || turn1 != 0; j = j+1;
                            :: j == 3 && k == 2 -> flag3 < k || turn2 != 0; j = j+1;
                            :: j == 3 && k == 3 -> flag3 < k || turn3 != 0; j = j+1;
                            fi;
                    ::  j >= 4 ->
                            break;
                    fi;
                od;
                k = k+1;
        ::  k >= 4-1 -> /* survived all N-1 rounds */
                break;
        od;
        
        mutex = mutex+1;
        assert(mutex == 1);
        mutex = mutex-1;

        flag0 = 0;
    od;
}

